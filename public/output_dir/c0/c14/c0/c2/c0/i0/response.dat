/* Override some defaults */
html body.page {
  background-color: #eee;
}
html body{
  padding-top: 60px;
}

body.page {
  padding-top: 40px; /* 40px to make the container go all the way to the bottom of the topbar */
}

body.page.sub-bar {
  padding-top: 80px; /* 40px to make the container go all the way to the bottom of the topbar */
}

.topbar ul.sub-bar .active > a, .nav.sub-bar .active a{
  /*background: #f2f2f2;*/
  background: rgba(0, 0, 0, 0.25);
}

body .container > footer p {
  text-align: center; /* center align it with the container */
}

body .container footer{
  padding-top: 30px;
}

body.page .container {
  /*width: 820px;*/
  /* downsize our container to make the content feel a bit tighter and more cohesive. NOTE: this removes two full columns from the grid, meaning you only go to 14 columns and not 16. */
}

/* The white background content wrapper */
body.page .container > .content {
  background-color: #fff;
  padding: 20px;
  margin: 0 -20px; /* negative indent the amount of the padding to maintain the grid system */
  -webkit-border-radius: 0 0 6px 6px;
     -moz-border-radius: 0 0 6px 6px;
          border-radius: 0 0 6px 6px;
  -webkit-box-shadow: 0 1px 2px rgba(0,0,0,.15);
     -moz-box-shadow: 0 1px 2px rgba(0,0,0,.15);
          box-shadow: 0 1px 2px rgba(0,0,0,.15);
}

/* Page header tweaks */
body.page .page-header {
  background-color: #f5f5f5;
  padding: 20px 20px 10px;
  margin: -20px -20px 20px;
}

body.page .page-header small{
  color: #757575;
  float: right;
  margin-top: 10px;
}

/* Styles you shouldn't keep as they are for displaying this base example only */
body.page .content .span10,
body.page .content .span4 {
